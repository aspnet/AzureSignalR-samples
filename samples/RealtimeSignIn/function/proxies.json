{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "signinproxy": {
            "matchCondition": {
                "route": "/signin",
                "methods": [
                    "GET",
                    "POST"
                ]
            },
            "backendUri": "https://%WEBSITE_HOSTNAME%/api/signin"
        },
        "scripts": {
            "matchCondition": {
                "route": "/scripts/{*rest}",
                "methods": [
                    "GET",
                    "OPTIONS"
                ]
            },
            "backendUri": "https://%BlobHost%/content/scripts/{rest}"
        },
        "images": {
            "matchCondition": {
                "route": "/images/{*rest}",
                "methods": [
                    "GET",
                    "OPTIONS"
                ]
            },
            "backendUri": "https://%BlobHost%/content/images/{rest}"
        },
        "default": {
            "matchCondition": {
                "route": "/",
                "methods": [
                    "GET",
                    "OPTIONS"
                ]
            },
            "backendUri": "https://%BlobHost%/content/index.html"
        }
    }
}